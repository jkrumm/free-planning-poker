ANALYTICS_SECRET_TOKEN=secret_token
DB_HOST=127.0.0.1
DB_PORT=33306
DB_USERNAME=fpp
DB_PASSWORD=FPP_DB_PW
DATA_DIR=./data
BEA_BASE_URL=https://your-backend-url.com
BEA_SECRET_KEY=your_backend_secret_key

# Sentry Configuration (SDK v2.42.1)
# Get your DSN from: https://sentry.io/settings/[your-org]/projects/[your-project]/keys/
FPP_ANALYTICS_SENTRY_DSN=https://your_sentry_dsn@sentry.io/your_project_id
SENTRY_ENVIRONMENT=production  # Options: production, staging, development
SENTRY_TRACES_SAMPLE_RATE=0.1  # 10% of transactions (0.0 to 1.0)
SENTRY_PROFILES_SAMPLE_RATE=0.1  # 10% of transactions with profiling (0.0 to 1.0)
# SENTRY_RELEASE=1.0.0  # Optional: Track releases (e.g., git commit hash or version)

# Logging
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARN, ERROR